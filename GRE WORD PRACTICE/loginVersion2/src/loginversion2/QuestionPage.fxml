<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="450.0" fitWidth="620.0" layoutX="2.0" layoutY="2.0" pickOnBounds="true">
        <image>
            <Image url="SourceImages/QuestionPage.jpg">
                
            </Image>
        </image>
      </ImageView>
      <Label fx:id="worDLabel" alignment="CENTER" layoutX="16.0" layoutY="51.0" prefHeight="97.0" prefWidth="600.0" text="WORDS WILL BE HERE" textFill="#ecf0f2" wrapText="true">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="meaningButton" layoutY="148.0" mnemonicParsing="false" onAction="#seeMeaningClick" prefHeight="50.0" prefWidth="620.0" text="SEE MEANING">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Button>
      <ProgressBar fx:id="masterBar" layoutX="19.0" layoutY="259.0" onMouseClicked="#masterbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <ProgressBar fx:id="reviewBar" layoutX="19.0" layoutY="313.0" onMouseClicked="#reviewbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <ProgressBar fx:id="learningBar" layoutX="19.0" layoutY="367.0" onMouseClicked="#learningbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <Label fx:id="masterLabel" layoutX="28.0" layoutY="231.0" prefHeight="28.0" prefWidth="585.0" text="You have mastered 0 out of 51 words">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="reviewLabel" layoutX="25.0" layoutY="287.0" prefHeight="28.0" prefWidth="585.0" text="review">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="learningLabel" layoutX="28.0" layoutY="341.0" prefHeight="28.0" prefWidth="585.0" text="learning">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ProgressIndicator fx:id="masterIndicator" layoutX="568.0" layoutY="259.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <ProgressIndicator fx:id="reviewIndicator" layoutX="568.0" layoutY="313.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <ProgressIndicator fx:id="learningIndicator" layoutX="568.0" layoutY="369.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <Label fx:id="statusLabel" alignment="CENTER" layoutX="459.0" layoutY="72.0" prefHeight="28.0" prefWidth="147.0" text="Word Status" textFill="#f7f9fc" wrapText="true">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="34.0" layoutY="39.0" mnemonicParsing="false" onAction="#logoutClick" prefHeight="22.0" prefWidth="72.0" text="Logout" />
   </children>
</AnchorPane>
