<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="480.0" fitWidth="626.0" pickOnBounds="true">
        <image>
            <Image url="SourceImages/AnswerPage.jpg">
                
            </Image>
        </image>
      </ImageView>
      <Button layoutX="5.0" layoutY="199.0" mnemonicParsing="false" onAction="#knownWord" prefHeight="42.0" prefWidth="616.0" text="I knew this word">
         <font>
            <Font name="Century Gothic Bold" size="17.0" />
         </font>
      </Button>
      <Button layoutX="5.0" layoutY="241.0" mnemonicParsing="false" onAction="#unKnownWord" prefHeight="42.0" prefWidth="616.0" text="I did not know this word">
         <font>
            <Font name="Century Gothic Bold" size="17.0" />
         </font>
      </Button>
      <ProgressIndicator fx:id="learningIndicator" layoutX="563.0" layoutY="432.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <ProgressIndicator fx:id="reviewIndicator" layoutX="563.0" layoutY="318.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <ProgressIndicator fx:id="masterIndicator" layoutX="565.0" layoutY="380.0" prefHeight="40.0" prefWidth="49.0" progress="0.0" />
      <Label fx:id="learningLabel" layoutX="28.0" layoutY="406.0" prefHeight="28.0" prefWidth="585.0" text="learning">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="reviewLabel" layoutX="25.0" layoutY="352.0" prefHeight="28.0" prefWidth="585.0" text="review">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="masterLabel" layoutX="25.0" layoutY="296.0" prefHeight="28.0" prefWidth="585.0" text="You have mastered 0 out of 51 words">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ProgressBar fx:id="learningBar" layoutX="19.0" layoutY="432.0" onMouseClicked="#learningbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <ProgressBar fx:id="reviewBar" layoutX="19.0" layoutY="378.0" onMouseClicked="#reviewbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <ProgressBar fx:id="masterBar" layoutX="19.0" layoutY="324.0" onMouseClicked="#masterbarClicked" prefHeight="28.0" prefWidth="549.0" progress="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ProgressBar>
      <Label fx:id="meaningLabel" alignment="CENTER" layoutX="10.0" layoutY="64.0" prefHeight="80.0" prefWidth="607.0" text="WORD MEANING" wrapText="true">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" alignment="CENTER" layoutX="504.0" layoutY="36.0" mnemonicParsing="false" onAction="#logoutClick" prefHeight="28.0" prefWidth="75.0" text="Logout" />
      <Button layoutX="19.0" layoutY="24.0" mnemonicParsing="false" onAction="#seePriority" text="Priority" />
      <Label fx:id="synonymLabel" alignment="CENTER" layoutX="11.0" layoutY="135.0" prefHeight="61.0" prefWidth="607.0" text="Synonym" wrapText="true">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Separator fx:id="lineSeparator" layoutX="11.0" layoutY="132.0" prefHeight="6.0" prefWidth="607.0" />
   </children>
</AnchorPane>
